<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_1_0.dtd">
<service-builder package-path="com.sbi.wms">
<author>Syed</author>
<namespace>SBI</namespace>

<entity name="Exclusif_WMS" local-service="true" remote-service="true">

<!-- PK fields -->
<column name="recId" type="long" primary="true"/>

<!--Audit fields  -->
<column name="name" type="String"/>
<column name="city" type="String"/>
<column name="mobileno" type="long"/>
<column name="email" type="String"/>
<column name="agree" type="String"/>

<!--Default fields  -->
<column name="companyId" type="long" />
<column name="groupId" type="long" />
<column name="userId" type="long" />
<column name="createDate" type="Date" />

</entity>

</service-builder>